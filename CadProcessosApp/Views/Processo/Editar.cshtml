@model CadProcessosApp.Models.Processo

@{
    ViewData["Title"] = "Editar Processo";
    ViewData["Action"] = "Editar";
}

<div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pb-2 mb-3 border-bottom">
    <h3>Editar Processo</h3>
    <div class="btn-toolbar mb-2 mb-md-0">
        <div class="btn-group mr-2">
            <a class="btn btn-sm btn-outline-danger" asp-action="Excluir" asp-route-id="@Model.Id">
                <i class="bi bi-trash"></i> Excluir Processo
            </a>
            <a class="btn btn-sm btn-outline-secondary" asp-action="Detalhes" asp-route-id="@Model.Id">
                <i class="bi bi-x"></i> Cancelar
            </a>
        </div>
    </div>
</div>

<partial name="~/Views/Shared/_Formulario.cshtml" model="Model" />

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    <script src="/js/municipios.js"></script>
}
