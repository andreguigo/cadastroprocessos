<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] Cadastro de Processos</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-icons/1.5.0/font/bootstrap-icons.min.css">

    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/CadProcessosApp.styles.css" asp-append-version="true" />
</head>
<body class="bg-light">
    <div class="container">
        <div class="py-3 text-center">
            <h2>Cadastro de Processos</h2>
            <p class="lead">Consulta e Acompanhamento de Processos Judiciais</p>
            @* <a class="navbar-brand col-sm-3 col-md-2 mr-0" asp-area="" asp-controller="" asp-action="Index">Cadastro de Processos</a> *@
        </div>

        <div class="row">
            @RenderBody()
        </div>
    </div>

    <footer class="my-5 pt-5 text-muted text-center text-small">
        <p class="mb-1">&copy; 2024 - Cadastro de Processos</p>
    </footer>
    
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
